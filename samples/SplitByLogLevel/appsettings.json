{
  "Logging": {
    "File": {
      "IncludeScopes": true,
      "EnsureBasePath": true,
      "BasePath": "Logs",
      "FallbackFileName": "warn+err.log",
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "InfoFile": {
      "IncludeScopes": true,
      "EnsureBasePath": true,
      "BasePath": "Logs",
      "FallbackFileName": "info.log",
      "LogLevel": {
        "Default": "Information"
      }
    },
    "LogLevel": {
      "Default": "Trace"
    }
  }
}
